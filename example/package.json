{
  "name": "carcer-example",
  "version": "0.2.0",
  "description": "an example service implemented in node",
  "main": "./src/index.js",
  "dependencies": {
    "grpc": "^1.9.1",
    "protobufjs": "^6.8.6"
  },
  "scripts": {
    "build": "yarn gen-json",
    "gen-json": "pbjs -o ./foo/plugin/foo.proto.json -t json ./foo/foo.proto",
    "build-docker": "docker build -t carcer/example .",
    "start": "cd ./werk && node ../../dist/carcer.js",
    "start-docker": "cd ./werk-docker && node ../../dist/carcer.js",
    "start-service": "cd ./foo && node index.js"
  }
}
